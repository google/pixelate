{
  "name": "pixelate",
  "version": "0.1.0",
  "author": {
    "name": "Max Vogler",
    "email": "maxvo@google.com",
    "url": "https://max.place"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test:watch": "ng test --watch=true",
    "lint": "npm run lint:formatting && npm run lint:code",
    "lint:formatting": "npx prettier --check .",
    "lint:code": "eslint --ext .ts --max-warnings=0 .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --max-warnings=0"
    ]
  },
  "dependencies": {
    "@angular/cdk": "^13.2.5",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.2.5",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "puppeteer": "^13.5.1",
    "typescript": "~4.5.2"
  }
}
